{% extends 'layout.html.twig' %}


{% block body %}
	{% if username %}
		<h5>{{ username }}</h5>
		<h6>{{ role }}</h6>
	{% endif %}


	<h3>
		Content Region
	</h3>
	<div class="inner-section section1 mt-1">Section 1</div>
	<div class="inner-section section2 mt-1">
		{% for r in result %}

			<div class="info__details-item">
				<li class='text-white'>{{ r.title }}</li>
				<li class='text-white'>{{ r.body }}</li>
			</div>
		{% endfor %}
	</div>
	<div class="inner-section section3 mt-1">Section 3</div>

{% endblock %}
